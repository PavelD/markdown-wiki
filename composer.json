{
    "name": "paveld/markdown-wiki",
    "description": "An extension of cebe/markdown library for parsing markdown to wiki text",
    "keywords": ["markdown", "wiki"],
    "homepage": "https://github.com/PavelD/markdown-wiki#readme",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Dobes",
            "email": "konference@lnx.cz",
            "role": "Creator"
        }
    ],
    "support": {
        "issues": "https://github.com/PavelD/markdown-wiki/issues",
        "source": "https://github.com/PavelD/markdown-wiki"
    },
    "require": {
        "cebe/markdown": "1.2.*",
        "lib-pcre": "*",
        "ext-mbstring": "*",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "paveld\\markdownwiki\\": ""
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests/MarkdownWikiTest.php"
    }
}
